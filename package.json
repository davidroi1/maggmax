{
  "name": "maggmax",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 24.13.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "npm run mocha",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.40",
    "@feathersjs/authentication": "^5.0.40",
    "@feathersjs/authentication-client": "^5.0.40",
    "@feathersjs/configuration": "^5.0.40",
    "@feathersjs/errors": "^5.0.40",
    "@feathersjs/feathers": "^5.0.40",
    "@feathersjs/koa": "^5.0.40",
    "@feathersjs/mongodb": "^5.0.40",
    "@feathersjs/schema": "^5.0.40",
    "@feathersjs/transport-commons": "^5.0.40",
    "@feathersjs/typebox": "^5.0.40",
    "mongodb": "^6.21.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.40",
    "@feathersjs/rest-client": "^5.0.40",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.3",
    "axios": "^1.13.5",
    "cross-env": "^10.1.0",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
